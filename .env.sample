# Database connection (used in dev/test)
PGUSER=postgres
PGPASSWORD=
PGHOST=localhost
PGDATABASE=net_auto_dev

# Phoenix secrets (override in runtime config)
SECRET_KEY_BASE=

# Example credentials (cred_ref: LAB_DEFAULT)
NET_AUTO_LAB_DEFAULT_USERNAME=netops
NET_AUTO_LAB_DEFAULT_PASSWORD=
# For key-based auth use either plain text or base64 (pick one)
NET_AUTO_LAB_DEFAULT_PRIVKEY=
NET_AUTO_LAB_DEFAULT_PRIVKEY_BASE64=
NET_AUTO_LAB_DEFAULT_PASSPHRASE=

# Retention/bulk knobs (optional overrides)
NET_AUTO_RUN_MAX_DAYS=30
NET_AUTO_RUN_MAX_BYTES=1073741824
NET_AUTO_RETENTION_CRON=@daily
